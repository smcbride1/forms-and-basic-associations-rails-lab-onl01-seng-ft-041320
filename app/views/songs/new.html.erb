<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title, "Title" %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :artist_name, "Artist" %>
  <%= f.text_field :artist_name %>
  <br>
  <%= f.label :collection_select, "Genre" %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <br>
  <%= f.label "Notes" %>
  <input name="song[note_contents][]" type="text" id="song_notes_1">
  <br>
  <input name="song[note_contents][]" type="text" id="song_notes_2">
  <br />

  <%= f.submit %>
<% end %>


